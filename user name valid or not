package project;
import java.util.Scanner;

public class Vaidornot {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Enter a username1: ");
        String username1 = scanner.nextLine();

        System.out.print("Enter a username2: ");
        String username2 = scanner.nextLine();

        // Use .equals() to compare the content of the strings
        if (username1.equals(username2)) {
            System.out.println("The usernames are the valid.");
        } 
        else {
            System.out.println("The usernames are not valid.");
        }

        scanner.close();
    }
}
